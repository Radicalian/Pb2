<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent" android:layout_height="match_parent"
    >


<RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
    android:background="@color/common_google_signin_btn_text_dark_pressed"
    >



<ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/sid1"
            android:id="@+id/imageView30"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

    <Button
        android:text="go to maharajganj branches"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/button21"
        android:textSize="16sp"
        android:textStyle="bold"
        android:layout_below="@+id/imageView33"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="33dp"
        android:onClick="onButtonClick21" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/sid4"
        android:id="@+id/imageView33"
        android:layout_marginTop="102dp"
        android:layout_below="@+id/imageView32"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/sid2"
        android:id="@+id/imageView31"
        android:layout_marginTop="6dp"
        android:layout_below="@+id/imageView30"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/sid3"
        android:id="@+id/imageView32"
        android:layout_marginTop="35dp"
        android:layout_below="@+id/imageView31"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
</RelativeLayout>
</ScrollView>